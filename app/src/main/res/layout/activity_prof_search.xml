<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_menu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/white"
    tools:context="comineashantrehan.linkedin.httpswww.ucourse.Menu">

    <ImageButton
        android:id="@+id/btnBack"
        android:layout_width="30dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="#FDFEFE"
        android:elevation="0dp"
        android:onClick="logout"
        app:srcCompat="?android:attr/homeAsUpIndicator" />
    <!--
    <ImageView
        app:srcCompat="@drawable/utoronto_logo"
        android:id="@+id/imageView3"
        android:layout_width="120dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_height="120dp" />
    -->
    <Button
        android:text="Search Professor"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:id="@+id/button7"

        android:layout_width="373dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_gravity="start"
        android:textAlignment="gravity"
        android:background="@drawable/my_button_bg"
        />

    <Button
        android:text="Add New Professor"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:id="@+id/button6"
        android:layout_width="373dp"
        android:layout_gravity="start"
        android:textAlignment="gravity"
        android:layout_below="@+id/button7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/my_button_bg"
        />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="180dp"
        android:text="Professor Name:" />

    <EditText
        android:id="@+id/profName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="110dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="170dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="" />

    <ListView
        android:id="@+id/profLists"
        android:layout_width="334dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="290dp"
        android:layout_height="376dp" />


</RelativeLayout>
